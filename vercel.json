{
  "rewrites": [
    {
      "source": "/_next/:path*",
      "destination": "https://yashnark.github.io/_next/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "https://yashnark.github.io/assets/:path*"
    },
    {
      "source": "/static/:path*",
      "destination": "https://yashnark.github.io/static/:path*"
    },

    {
      "source": "/game-hub",
      "destination": "https://yashnark.github.io/game-hub"
    },
    {
      "source": "/game-hub/:path*",
      "destination": "https://yashnark.github.io/game-hub/:path*"
    },
    {
      "source": "/SimpleDex",
      "destination": "https://yashnark.github.io/SimpleDex"
    },
    {
      "source": "/SimpleDex/:path*",
      "destination": "https://yashnark.github.io/SimpleDex/:path*"
    }
  ]
}
